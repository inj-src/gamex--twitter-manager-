{
  "name": "wxt-react-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "smoke:classifier": "ts-node ./scripts/smoke-classifier.ts",
    "smoke:storage": "ts-node ./scripts/storage-smoke.ts",
    "smoke": "npm run smoke:classifier && npm run smoke:storage",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@wxt-dev/module-react": "^1.1.3",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  }
}
